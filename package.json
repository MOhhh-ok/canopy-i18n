{
  "name": "canopy-i18n",
  "version": "0.6.0",
  "description": "The Type-Safe i18n library that your IDE will Love",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "prepublishOnly": "pnpm run build",
    "type-check": "tsc -p . --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "release": "release-it"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.8",
    "@types/node": "^25.0.10",
    "release-it": "^19.2.4",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "keywords": [
    "i18n",
    "internationalization",
    "localization",
    "l10n",
    "messages",
    "template"
  ],
  "engines": {
    "node": ">=20"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mohhh-ok/canopy-i18n"
  }
}
