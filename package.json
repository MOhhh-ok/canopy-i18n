{
  "name": "canopy-i18n",
  "version": "0.0.5",
  "description": "A tiny, type-safe i18n helper",
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "prepublishOnly": "pnpm run build",
    "check": "tsc -p . --noEmit"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.6",
    "@types/node": "^24.3.3",
    "typescript": "^5.9.2"
  },
  "keywords": [
    "i18n",
    "internationalization",
    "localization",
    "l10n",
    "messages",
    "template"
  ],
  "engines": {
    "node": ">=20"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MOhhh-ok/canopy-i18n"
  }
}